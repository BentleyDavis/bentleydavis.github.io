---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - nav
    - tagList
    - draft
  addAllPagesToCollections: true
eleventyComputed:
  title: "{{ tag }} - Bentley Davis"
permalink: /{{ tag.toLowerCase() }}/
layout:
---
{% block content %}
  <div class="content">

    <h1>{{ tag }}</h1>

    {% set items = collections[ tag ] %}
    {% include 'grid.njk' %}

  </div>
{% endblock %}

{% block styles %}
    {% include 'grid.css' %}
{% endblock %}


{% extends "_includes/base.njk" %}